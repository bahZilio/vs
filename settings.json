{
  "editor.fontFamily": "Fira Code",
  "editor.suggestSelection": "first",
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    "*.tsx": "typescriptreact",
    ".prettierrc": "json"
  },
  "editor.rulers": [80, 120],
  "material-icon-theme.folders.associations": {
    "sdk": "components",
    "analyzer": "core",
    "worker": "core",
    "agent": "queue",
    "devops": "server",
    "backend": "api",
    "npm": "packages",
    "grpc": "pipe",
    "kube": "kubernetes",
    "websockets": "pipe",
    "implementations": "core",
    "main": "config",
    "protos": "pipe",
    "entities": "class",
    "install": "config",
    "policies": "secure",
    "kafka": "scripts",
    "migrations": "tools",
    "schemas": "class",
    "backoffice": "admin",
    "example": "other",
    "events": "log",
    "assets": "app",
    "app": "layout",
    "useCases": "controller",
    "typeorm": "database",
    "_shared": "shared",
    "mappers": "meta",
    "fakes": "mock",
    "modules": "components",
    "subscribers": "messages",
    "domain": "class",
    "eslint-config": "tools",
    "infra": "app",
    "websocket": "event",
    "swagger": "class",
    "microservices": "controller",
    "_server": "server",
    "dtos": "typescript",
    "http": "container",
    "providers": "include",
    "repositories": "mappings",
    "_deploy": "dist",
    "knowledge": "docs"
  },
  "terminal.integrated.fontSize": 14,
  "editor.accessibilitySupport": "off",
  "editor.lineHeight": 26,
  "material-icon-theme.files.associations": {
    "tsconfig.json": "tune",
    "*.webpack.js": "webpack",
    "*.proto": "3d",
    "ormconfig.json": "database",
    "docker-compose-dev.yml": "docker",
    "dockerfile-dev": "docker"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "debug.disassemblyView.showSourceCode": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.log": "verbose",
  "editor.tabSize": 2,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "[scss]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 16,
  "tabnine.experimentalAutoImports": true,
  "workbench.iconTheme": "material-icon-theme",
  "aws.profile": "profile:default",
  "terminal.integrated.showExitAlert": false,
  "aws.onDefaultRegionMissing": "ignore",
  "editor.semanticHighlighting.enabled": false,
  "git.autofetch": true,
  "json.schemas": [],
  "cSpell.language": "en,pt",
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "window.menuBarVisibility": "toggle",
  "redhat.telemetry.enabled": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  "material-icon-theme.activeIconPack": "nest",
  "editor.fontLigatures": true,
  "explorer.compactFolders": false,
  "cSpell.userWords": [
    "amala",
    "Antd",
    "Autônomos",
    "Boleto",
    "browserslist",
    "classname",
    "CNPJ",
    "conexoes",
    "conosco",
    "Contatado",
    "Contate",
    "contato",
    "creci",
    "cryptojs",
    "deletada",
    "Deletar",
    "Desenvolvedor",
    "Detecção",
    "DTOS",
    "envalid",
    "Faturamento",
    "Florianópolis",
    "fullscreen",
    "hext",
    "hostinger",
    "hotmart",
    "icomoon",
    "Icone",
    "Incorporadoras",
    "Invillia",
    "lazyload",
    "logado",
    "Loteadoras",
    "Lucratividade",
    "middlewares",
    "Nmap",
    "noindex",
    "ntadev",
    "orderfast",
    "pentest",
    "Pentests",
    "picpay",
    "politica",
    "Porcentagem",
    "Postagem",
    "Postagens",
    "preconnect",
    "predeploy",
    "Pressable",
    "profisas",
    "Rabbitmq",
    "Roadmap",
    "SENDINBLUE",
    "SIEM",
    "tiktok",
    "tipoassunto",
    "TOMTICKET",
    "tramontina",
    "trivago",
    "typegoose",
    "Unimed",
    "usecases",
    "USERID",
    "whois",
  ],
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "git.ignoreRebaseWarning": true,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "remote.SSH.remotePlatform": {
    "browzilio.dashboard-security.draft---distracted-northcutt.csb": "linux"
  },
  "workbench.editor.untitled.hint": "hidden",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "window.zoomLevel": 1,
  "editor.unicodeHighlight.allowedCharacters": {
    "ˆ": true,
    "˜": true
  },
  "[python]": {
    "editor.formatOnType": true
  }
}
